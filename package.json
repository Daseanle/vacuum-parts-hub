{
  "name": "vacuum-parts-hub",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "docker:build": "docker build -t vacuum-parts-hub .",
    "docker:run": "docker run -p 3000:3000 vacuum-parts-hub",
    "docker:compose": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "deploy": "npm run build && vercel --prod",
    "health-check": "curl -f http://localhost:3000 || exit 1"
  },
  "dependencies": {
    "react": "^18",
    "@vercel/analytics": "^1.2.2",
    "react-dom": "^18",
    "next": "14.1.0",
    "lucide-react": "^0.344.0"
  },
  "devDependencies": {
    "typescript": "^5",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/jest-dom": "^6.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "@types/jest": "^29.5.0"
  }
}
